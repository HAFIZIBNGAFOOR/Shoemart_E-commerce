                <div class="row d-flex align-items-center justify-content-center h-100">
        
                    <div class=" w-100 col-md-6 col-lg-6 col-xl-6 offset-xl-1">
        
                        <form action="/admin/addproduct" method="post" enctype="multipart/form-data">
                            <h1 class="pb-3 font-weight-bold">Add Product</h1>
        <div class="container">
          <div class="row">
            <div class="col-6">
                            <div class="form-outline mb-4">
                                <input type="text" id="name" name="name" class="form-control form-control-lg border-dark"
                                    placeholder="Product Name" />
                            </div>
        
        
                            <div class="form-outline mb-4">
                                <input type="number" id="price" name="price" pattern=".{1,6}"
                                    class="form-control form-control-lg border-dark" placeholder="Price" />
                            </div>
        
                            <div class="form-outline mb-4">
                                <textarea type="text" rows="6" id="description" name="description"
                                    class="form-control form-control-lg border-dark" placeholder="Description"></textarea>
                            </div>
        
                            
                            </div>
        <div class="col-6">
          <div class="form-outline mb-4">
            <input type="number" id="stock" name="stock" class="form-control form-control-lg border-dark"
                placeholder="stock" />

        </div>
                            <div class="form-outline mb-4">
                              
                                <select name="category" class="form-control form-control-lg border-dark"  >
                                    <option>choose the category</option>
                                    <%for(let i=0;i <category.length; i++){%>
                                    
                                    <option value=<%=category[i]._id%>><%=category[i].category%></option>
                                    <%}%>
                                </select>
                               
                            </div>
                           
        
                           
        
                            <div class="form-outline mb-4">
                                <input type="file" class="form-control form-control-lg border-dark" name="image" id="image"
                                    accept="image/*" multiple/>
                            </div>
                            </div>
                            </div>
                          </div>
        
                            <div class="d-flex justify-content-around align-items-center mb-3">
                                <h6>Want to go back<a href="/admin/products"> click here</a></h6>
                            </div>
                            
                            <% if(typeof message !== 'undefined'){  %>
                            
                                <h5 class="text-danger text-center "><%= message %> </h5>
                            
                                <%
                            }
                            
                            %>
        
                            <button type="submit" class="btn btn-success btn-lg btn-block w-100 border-dark mb-5">Add
                                Product</button>
        
                        </form>
                    </div>
                </div>
            </div>
        </section>

